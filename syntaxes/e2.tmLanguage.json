		},
		"types": {
			"patterns": [{
				"name": "entity.name.type.e2",
				"match": "void|number|vector4|vector2|vector|angle|string|entity|array|table|ranger|bone|wirelink|gtable"
			}]
		},
		"number": {
			"patterns": [{
				"name": "constant.numeric.e2",
				"match": "\\b[0-9\\.]+\\b|true|false"
			}]
		},
		"function": {
			"patterns": [{
				"name": "keyword.e2",
				"match": "function ([a-z]+) ([A-Za-z0-9]+)",
				"captures": {
					"1": {
						"name": "entity.name.type.e2"
					},
					"2": {
						"name": "entity.name.function.e2"
					}
				}
			}
		]
		},
		"option": {
			"patterns": [{
				"name": "keyword.other.unit.e2",
				"match": "@inputs|@outputs|@persist|@trigger|@model|@autoupdate"
			},
			{
				"name": "keyword.other.unit.e2",
				"match": "(@name) (.+)",
				"captures": {
					"1": {
						"name": "keyword.other.unit.e2"
					},
					"2": {
						"name": "string"
					}
				}
			}
		]
		},
		"comment": {
			"patterns": [
			{
				"name": "comment.line.e2",
				"match": "#.+"
			}, 
			{
				"name": "comment.block.e2",
				"begin": "#\\[", 
				"end": "\\]#"
			}]
		},
		"strings": {
			"name": "string.quoted.double.skript",
			"begin": "\"",
			"end": "\"",
			"patterns": [{}]
		}
	},
	"scopeName": "source.e2"
